{
    "name": "jiannius/atom-livewire",
    "description": "Boilerplate for Larave-Livewire",
    "type": "library",
    "require": {
        "barryvdh/laravel-debugbar": "^3",
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.6",
        "intervention/image": "^2.7",
        "laravel/socialite": "^5.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.0",
        "orchestra/testbench": "^8.0",
        "rap2hpoutre/fast-excel": "^5.0",
        "torann/geoip": "^3.0",
        "jenssegers/agent": "^2.0",
        "sentry/sentry-laravel": "^3",
        "stripe/stripe-php": "^10.0",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "laravel/sanctum": "^3.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jiannius\\Atom\\": "src/"
        }
    },
    "authors": [
        {
            "name": "TJ",
            "email": "tj@jiannius.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Jiannius\\Atom\\Providers\\AtomServiceProvider",
                "Jiannius\\Atom\\Providers\\AtomBladeServiceProvider",
                "Jiannius\\Atom\\Providers\\AtomCommandServiceProvider",
                "Jiannius\\Atom\\Providers\\AtomComponentServiceProvider",
                "Jiannius\\Atom\\Providers\\AtomGateServiceProvider",
                "Jiannius\\Atom\\Providers\\AtomEmailServiceProvider",
                "Jiannius\\Atom\\Providers\\AtomSocialiteServiceProvider",
                "Jiannius\\Atom\\Providers\\AtomLivewireServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": false,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
